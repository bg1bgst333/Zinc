<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bgstation0.android.application.zinc"
    android:versionCode="8"
    android:versionName="0.0.0.0 #8 (Develop/Minami-Urawa)" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.bgstation0.android.application.zinc.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bgstation0.android.application.zinc.BookmarkActivity"
            android:label="@string/activity_bookmark_title_text"
            android:parentActivityName="com.bgstation0.android.application.zinc.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.bgstation0.android.application.zinc.MainActivity" />
        </activity>
    </application>

</manifest>